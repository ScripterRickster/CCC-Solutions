#include <iostream>
#include <vector>
using namespace std;
int main() {
  vector<string> aliens = {};
  int a,e;
  //a = antennaes, e = eyes
  cin >> a >> e;

  if(a>=3 && e<=4){
    aliens.push_back("TroyMartian");
  }

  if(a<=6 && e>=2){
    aliens.push_back("VladSaturnian");
  }

  if(a<=2 && e<=3){
    aliens.push_back("GraemeMercurian");
  }

  

  for(int i=0; i<aliens.size();i++){
    cout << aliens[i] << endl;
  }
}
